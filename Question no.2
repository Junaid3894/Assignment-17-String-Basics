/*2. Write a program to count the occurrence of a given character in a given string.*/

#include<stdio.h>
int main()
{
char str[20];
char find;
printf("Enter the string :");
fgets(str,20,stdin);
printf("Enter the character which you want find the occurrence in a given string :");
scanf("%c",&find);
for(int i=0;str[i];i++){
    if(str[i]==find){
        printf("Your character is found on %d element",i+1);
        break;
    }
}
return 0;
}
