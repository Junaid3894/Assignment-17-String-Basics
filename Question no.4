/*4. Write a program to convert a given string into uppercase*/

#include<stdio.h>
int main()
{
char str[20];
printf("Enter the string :");
fgets(str,20,stdin);
for(int i=0;str[i];i++){
   if(str[i]>='a' && str[i]<='z'){
   str[i]=str[i]-32;
    }
}
printf("Your string after convert uppercase :%s",&str[0]);
return 0;
}
